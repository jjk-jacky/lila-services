#!@BINDIR@/execlineb -P
aa-service
backtick -n device { ifte
  { cat /etc/hdparm/${INSTANCE}/device }
  { echo /dev/disk/by-id/${INSTANCE} }
  aa-test -r /etc/hdparm/${INSTANCE}/device
}
importas -u device device
backtick -ni options { cat /etc/hdparm/${INSTANCE}/options }
importas -us options options
hdparm ${options} ${device}
